{
  "extends": "eslint-config-airbnb/base",
  "rules": {
    "comma-dangle": 0,
    "eol-last": 0,
    "indent": [2, 4, {
      "VariableDeclarator": 1
    }],
    "max-len": [1, 140, 4, {
      "ignoreComments": true,
      "ignoreUrls": true
    }],
    "no-use-before-define": 0,
    "spaced-comment": 0
  }
}